{
  "type": "object",
  "required": [
    "created_at",
    "id",
    "slug",
    "status",
    "submitted_on",
    "updated_at",
    "user_id"
  ],
  "properties": {
    "case_number": { "type":  "string"},
    "copay_cents": { "type": "integer", "minimum": 0 },
    "copay_currency": { "$ref": "currency_or_null.json" },
    "copay_frequency": {
      "anyOf": [
        { "$ref":"copay_frequencies.json" },
        { "type": "null" }
      ]
    },
    "created_at": { "type": "string", "format": "date-time" },
    "effective_on": { "$ref": "currency_or_null.json" },
    "expires_on": { "$ref": "currency_or_null.json" },
    "id": { "type": "string", "format": "uuid" },
    "notified_on": { "$ref": "currency_or_null.json" },
    "slug": { "type": "string" },
    "status": { "$ref":"case_statuses.json" },
    "submitted_on": { "type": "string", "format": "date" },
    "updated_at": { "type": "string", "format": "date-time" },
    "user_id": { "type": "string", "format": "uuid" }
  }
}
